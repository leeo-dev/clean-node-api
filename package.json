  "bugs": {
    "url": "https://github.com/leeodesign/clean-node-api/issues"
  },
  "homepage": "https://github.com/leeodesign/clean-node-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "standard": "^16.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
